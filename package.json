{
  "name": "maranatha-sender",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "ensure-production": "npx json -I -f app.config.json -e 'this.environment=\"prod\"'",
    "build-laravel": "vite build --base=/app/ --outDir=../resources/ionic --emptyOutDir",
    "local-build-laravel": "vite build --base=/app/ --outDir=../maranatha-sender/resources/ionic --emptyOutDir",
    "bundle-laravel": "vite build --outDir=../resources/ionic/.bundles --emptyOutDir && npm run bundler-live-updates --path ../resources/ionic/.bundles",
    "local-bundle-laravel": "vite build --outDir=../maranatha-sender/resources/ionic/.bundles --emptyOutDir && npm run bundler-live-updates --path ../maranatha-sender/resources/ionic/.bundles",
    "bundle-live-update": "vite build --emptyOutDir && npm run bundler-live-updates --path ./dist",
    "bundle-web-update": "vite build --base=/app/ --emptyOutDir && npm run bundler-live-updates --path ./dist",
    "build-sourcemap": "vite build --sourcemap",
    "local-build-sourcemap": "export NODE_OPTIONS=--max-old-space-size=4096 && vite build --sourcemap",
    "cli": "npx tsx ./app.build.ts",
    "bundler-live-updates": "npx tsx ./.liveupdate-bundler.ts",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org prolux-inc --release maranathasender@$(node -p \"require('./app.config.json').versioning.version\") --project maranatha-invoices-native ./dist && sentry-cli sourcemaps upload --org prolux-inc --release maranathasender@$(node -p \"require('./app.config.json').versioning.version\") --dist $(node -p \"require('./app.config.json').versioning.version\").$(node -p \"require('./app.config.json').versioning.build\") --project maranatha-invoices-native ./dist"
  },
